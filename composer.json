{
    "name": "vkovic/laravel-model-meta",
    "description": "Laravel model meta storage",
    "keywords": [
        "laravel",
        "model",
        "meta",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir KoviÄ‡",
            "email": "vlada.kovic@gmail.com"
        }
    ],
    "autoload": {
